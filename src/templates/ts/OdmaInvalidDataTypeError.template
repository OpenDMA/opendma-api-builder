/**
 * OdmaInvalidDataTypeError
 * 
 * Thrown when the provided data type does not match the expected data type.
 */
export class OdmaInvalidDataTypeError extends OdmaError {

    /**
     * Create a new OdmaInvalidDataTypeError.
     * 
     * @param message - The error message.
     */
    constructor(message: string = "Invalid data type provided") {
        super(message);
    }

}
